#include<stdio.h>
#include<math.h>
int main(){
	float x1,y1,x2,y2,x,y, dis, distance;
	int rect[3][3];
	printf("Enter x1: ");
	scanf("%f", &x1);
	printf("Enter x2: ");
	scanf("%f", &x2);
	printf("Enter y1:");
	scanf("%f", &y1);
	printf("Enter y2: ");
	scanf("%f", &y2);
	if(x1<=3 && x2<=3 && y1<=3 && y2<=3){
		dis = (x2-x1)*(x2-x1) + (y2-y1)*(y2-y1);
		distance = sqrt(dis);
		printf("The distance between two points is: %f",distance); 
	}
	else
	printf("Out of boundary");
	
}
